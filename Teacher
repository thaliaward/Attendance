public class Teacher
{
	String login, password, name;
	ArrayList<Classroom> rooms = new ArrayList<Classroom>(0);
	
	public Teacher()
	{
		login = "Login";
		password = "Password";
		name = "Name";
		fillRooms();
	}
	public Teacher(String log, String pWord, String teachName)
	{
		login = log;
		password = pWord;
		name = teachName;
		fillRooms();
	}
	
	public void fillRooms()
	{
		
	}
	
	public void setLogin(String log)
	{
		login = log;
	}
	public void setPassword(String pWord)
	{
		password = pWord;
	}
	public void setName(String teacherName)
	{
		name = teacherName;
	}
}
